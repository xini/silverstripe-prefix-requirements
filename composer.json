{
	"name": "innoweb/silverstripe-prefix-requirements",
	"description": "Adds a file hash prefix to all requirements. Replaces the default ?{mtime} suffix to make the requirements fully cacheable.",
	"type": "silverstripe-vendormodule",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "caching", "requirements", "prefix"],
	"homepage": "http://github.com/xini/silverstripe-prefix-requirements",
	"authors": [{
		"name": "Florian Thoma",
		"homepage": "http://www.innoweb.com.au"
	}],
	"require":
	{
		"silverstripe/framework": "^5",
		"webmozart/glob": "^4.6.0"
	},
	"autoload": {
		"psr-4": {
			"Innoweb\\PrefixRequirements\\": "src/"
		}
	},
	"replace": {
		"xini/silverstripe-prefix-requirements": "self.version"
	}
}
